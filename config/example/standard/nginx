server {
    listen 80;
    listen [::]:80;

    mymap.com;

    location / {
        proxy_pass              http://127.0.0.1:8080/;
        proxy_set_header        Host $host;
        proxy_set_header        X-Real-IP $remote_addr;
        proxy_request_buffering off;
        proxy_buffering         off;
        proxy_set_header        Connection keep-alive;
    }
}
